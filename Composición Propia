int posicionX;
int posicionY;

void setup(){
  size(800,800);
}
void draw(){
background(#07B5F7);
fill(#FFF300);
ellipse(400-posicionX,500-posicionY,200,200);
posicionY++;
if(posicionY==400){
  posicionY=0;
  }
  fill(#07F709);
  rect(0,600,800,600);
  line(0,600,800,600);
  fill(#FFFFFF);
  rect(50,500,100,100);
  fill(#B4782E);
  triangle(25,500,175,500,100,400);
  rect(90,550,25,50);
  fill(#000000,100); 
  quad(25, 650, 125,650,150,600,50,600);
}
